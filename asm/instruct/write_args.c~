#include "my_printf.h"
#include "asm.h"

int		write_args(t_instruct *instructs)
{
  int		i;

  i = 0;
  while (instructs && ++i)
    {
      
      instructs = instructs->next;
      my_printf("%d\n", i);
    }
}
